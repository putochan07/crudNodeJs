<% include ../partials/header %>
<br>
	<form action ="http://localhost:4000/event/edit/<%=items[0].id %>" method ="post" class="col s12">
		
		<div class="container">
		<div class="form-group">
		<label for="exampleInputEmail1">ID</label>
		<input type="number" class="form-control" name="id" value = "<%=items[0].id%>"/>
		</div>
		
		<div class="form-group">
		<label for="exampleInputEmail1">Full Name</label>
		<input type="text" class="form-control" name="name" value = "<%=items[0].name%>"/>
		</div>
		
		<div class="form-group">
		<label for="exampleInputEmail1">Home Address</label>
		<input type="text" class="form-control" name="homeadd" value = "<%=items[0].homeadd%>"/>
		</div>
	
		<div class="form-group">
		<label for="exampleInputEmail1">Email address</label>
		<input type="text" class="form-control" name="emailadd" value = "<%=items[0].emailadd%>"/>
		</div>
		
	
		<button type="submit" class="btn btn-default" name="submit">Submit</button>
		
		</div>
		
	</form>
	
<% include ../partials/footer %>